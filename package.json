{
  "name": "gringott",
  "version": "1.0.0",
  "description": "A Discord bot to perform simple who owes whom tasks'",
  "main": "build/client.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm/transpile-only src/client.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/client.ts",
    "serve": "node build/client.js",
    "prod": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
