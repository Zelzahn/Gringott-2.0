  "bugs": {
    "url": "https://github.com/Zelzahn/Gringott/issues"
  },
  "homepage": "https://github.com/Zelzahn/Gringott#readme",
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^3.0.0",
    "@discordx/utilities": "^5.0.0",
    "@kurkle/color": "^0.1.9",
    "chalk": "^4.1.0",
    "discord.js": "^14.1.2",
    "discordx": "^11.0.4",
    "eslint": "^7.21.0",
    "mongoose": "^6.0.14",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "table": "^6.7.5"
